{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/*/**.ts",
      "use": "@vercel/ts-node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/src/app.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}